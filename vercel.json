{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
  {
    "src": "/public/(.*)",
    "dest": "/$1"
  },
  {
    "src": "/assets/(.*)", 
    "dest": "/assets/$1" 
  },
  {
    "src": "/(.*)",
    "dest": "/index.html"
  }
]
}
